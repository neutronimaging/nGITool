<reconstructor>
    <userinformation>
        <operator>John Doe</operator>
        <instrument>NI beamline</instrument>
        <projectnumber>P20160000</projectnumber>
        <sample>Unknown item</sample>
        <comment>No comment</comment>
    </userinformation>
    <system>
        <memory>8000</memory>
        <loglevel>message</loglevel>
    </system>
    <projections>
	<beamgeometry>parallel</beamgeometry>
        <dims>100 100</dims>
        <resolution>0.1 0.1</resolution>
        <binning>1</binning>
        <margin>2</margin>
        <firstindex>1</firstindex>
        <lastindex>100</lastindex>
        <projectionstep>1</projectionstep>
        <repeatline>false</repeatline>
        <scantype>sequential</scantype>
        <imagetype>projections</imagetype>
        <center>50</center>
	    <sod>100</sod>
	    <sdd>100</sdd>
	    <direction>RotationDirCW</direction>
	    <ppoint>50.0 50.0 </ppoint>
        <translation>false</translation>
        <tiltangle>0</tiltangle>
        <tiltpivot>0</tiltpivot>
        <correcttilt>false</correcttilt>
        <filemask>proj_####.fits</filemask>
        <path></path>
        <referencepath></referencepath>
        <obfilemask>ob_####.fits</obfilemask>
        <obfirstindex>1</obfirstindex>
        <obcount>5</obcount>
        <dcfilemask>dc_####.fits</dcfilemask>
        <dcfirstindex>1</dcfirstindex>
        <dccount>5</dccount>
        <roi>0 0 99 99 </roi>
        <projroi>0 0 99 99</projroi>
        <doseroi>0 0 10 10</doseroi>
        <scanarc>0 360</scanarc>
        <rotate>imagerotatenone</rotate>
        <flip>imageflipnone</flip>
    </projections>
    <matrix>
        <dims>100 100 32</dims>
        <rotation>0</rotation>
        <serialize>false</serialize>
        <path>C:\Users\kaestner\</path>
        <matrixname>slice_####.tif</matrixname>
        <filetype>TIFF16bits</filetype>
        <firstindex>0</firstindex>
        <grayinterval>0 1</grayinterval>
        <useroi>false</useroi>
        <roi>40 40 60 60</roi>
	<voxelsize>0 0 0 </voxelsize>
	<usevoi>false</usevoi>
	<voi>0 0 0 0 0 0</voi>
    </matrix>
    <processchain>
        <preprocessing>
            <module>
                <modulename>FullLogNorm</modulename>
                <sharedobject>@executable_path\StdPreprocModules.dll</sharedobject>
                <active>true</active>
                <parameters>
                    <referenceaverage>ImageWeightedAverage</referenceaverage>
                    <uselut>false</uselut>
                    <usenormregion>true</usenormregion>
                </parameters>
            </module>
              <module>
                <modulename>SpotClean2</modulename>
                <sharedobject>@executable_path\StdPreprocModules.dll</sharedobject>
                <active>false</active>
                <parameters>
                    <detectionmethod>ring</detectionmethod>
                    <gamma>0.017</gamma>
                    <iterations>1</iterations>
                    <maxarea>20</maxarea>
                    <maxlevel>5</maxlevel>
                    <minlevel>0</minlevel>
                    <sigma>0.0005</sigma>
                </parameters>
            </module>
            <module>
                <modulename>MedianMixRingClean</modulename>
                <sharedobject>@executable_path\StdPreprocModules.dll</sharedobject>
                <active>false</active>
                <parameters>
                    <threshold>0.007</threshold>
                    <width>0.0005</width>
                </parameters>
            </module>
            <module>
                <modulename>ProjectionFilterSingle</modulename>
                <sharedobject>@executable_path\StdPreprocModules.dll</sharedobject>
                <active>true</active>
                <parameters>
                    <biasweight>0.1</biasweight>
                    <cutoff>0.5</cutoff>
                    <filtertype>Hamming</filtertype>
                    <order>1</order>
                    <paddingdoubler>2</paddingdoubler>
                    <usebias>true</usebias>
                </parameters>
            </module>
        </preprocessing>
        <backprojector>
            <module>
                <modulename>MultiProjBPparallel</modulename>
                <sharedobject>@executable_path\StdBackProjectors.dll</sharedobject>
                <active>true</active>
                <parameters>
                    <ProjectionBufferSize>16</ProjectionBufferSize>
                    <SliceBlock>32</SliceBlock>
                    <SubVolume>1 1</SubVolume>
                </parameters>
            </module>
        </backprojector>
    </processchain>
</reconstructor>
